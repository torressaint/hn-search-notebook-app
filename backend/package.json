{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for hn-search-notebook application",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "debug": "nodemon --inspect",
    "test": "jest --runInBand --verbose --forceExit",
    "tdd": "jest --runInBand --verbose --watch",
    "dbcreate": "sequelize db:create",
    "dbup": "sequelize db:migrate",
    "dbdrop": "sequelize db:drop",
    "dbclear": "sequelize db:migrate:undo:all"
  },
  "author": "Rafał Świątek",
  "license": "ISC",
  "devDependencies": {
    "jest": "^29.6.1",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.3"
  }
}
